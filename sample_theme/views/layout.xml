<!-- views/layout.xml -->

<odoo>

    <template id="website_new_page" name="Website New Page">
        <t t-call="website.layout">
            <div id="wrap" class="oe_structure oe_empty">
                <section class="s_text_block pt40 pb40 o_colored_level" data-snippet="s_text_block" data-name="Text" style="background-image: none;">
                    <div class="container s_allow_columns">
                        <p class="o_default_snippet_text">This is the new page</p>
                        <p class="o_default_snippet_text">The content is translated to two languages</p>
                    </div>
                </section>
            </div>
        </t>
    </template>

</odoo>
